# .env.example - Copy this to .env and update values

# Node environment
NODE_ENV=development

# Server port
PORT=3000

# PostgreSQL Database URL
# Format: postgresql://username:password@host:port/database
DATABASE_URL="postgresql://order_user:123@localhost:5432/order_execution"

# Redis Configuration
REDIS_URL="redis://127.0.0.1:6379"
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# Logging
LOG_LEVEL=debug

# For production, use:
# NODE_ENV=production
# LOG_LEVEL=info
# PORT=3000
# DATABASE_URL=postgresql://username:password@prod-host:5432/prod-db
# REDIS_URL=redis://prod-redis-host:6379
