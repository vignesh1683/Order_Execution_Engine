{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "Limit order execution engine with DEX routing and WebSocket updates",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --forceExit --detectOpenHandles",
    "test:coverage": "jest --coverage --forceExit",
    "test:watch": "jest --watch",
    "db:reset": "npx prisma migrate reset --force",
    "db:push": "npx prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "prisma:generate": "npx prisma generate"
  },
  "keywords": [
    "solana",
    "dex",
    "order-execution",
    "websocket",
    "limit-order"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/static": "^8.3.0",
    "@fastify/websocket": "^11.0.0",
    "@prisma/client": "^5.8.0",
    "bullmq": "^5.10.0",
    "fastify": "^5.2.0",
    "ioredis": "^5.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "prisma": "^5.8.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
